{
  "name": "Postory",
  "version": "0.1",
  "description": "Postory extension - track packages",
  "permissions": ["storage"],
  "background": {
    "scripts": ["poll.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["*://www.fedex.com/apps/fedextrack/*"],
      "js": ["fedex.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
